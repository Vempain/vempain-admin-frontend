{
  "name": "@vempain/vempain-admin-frontend",
  "version": "0.9.0",
  "description": "Vempain Admin frontend",
  "author": "Paul-Erik Törrönen",
  "homepage": "https://vempain.poltsi.fi",
  "license": "GPL-2.0",
  "packageManager": "yarn@4.9.2",
  "scripts": {
    "prebuild": "node generateBuildInfo.js",
    "prestart": "node generateBuildInfo.js",
    "start": "env-cmd -f .env.local vite",
    "start-combined": "env-cmd -f .env.combined vite --port 4000",
    "build": "yarn prebuild && tsc && env-cmd -f .env.prod vite build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage .",
    "test:debug": "jest --inspect-brk --runInBand --no-cache",
    "preview": "vite preview",
    "build:production": "yarn prebuild && env-cmd -f .env.prod vite build"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^22.13.5",
    "@types/react-highlight-words": "^0.20.0",
    "@vempain/vempain-auth-frontend": "^0.9.8",
    "@vitejs/plugin-react": "^5.1.0",
    "antd": "^5.27.6",
    "axios": "^1.12.2",
    "dayjs": "^1.11.18",
    "dompurify": "^3.3.0",
    "env-cmd": "^11.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-highlight-words": "^0.21.0",
    "react-router-dom": "^7.9.4",
    "web-vitals": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^4.2.0",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vempain/vempain-auth-frontend.git"
  }
}
