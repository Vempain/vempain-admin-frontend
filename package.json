{
  "name": "vempain-admin-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-highlight-words": "^0.20.0",
    "@vitejs/plugin-react": "^4.3.4",
    "antd": "^5.24.2",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.4",
    "env-cmd": "^10.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-highlight-words": "^0.21.0",
    "react-router-dom": "^7.2.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vite-tsconfig-paths": "^5.1.4",
    "web-vitals": "^3.5.2"
  },
  "scripts": {
    "prebuild": "node generateBuildInfo.js",
    "prestart": "node generateBuildInfo.js",
    "start": "env-cmd -f .env.local vite",
    "build": "tsc && env-cmd -f .env.prod vite build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage .",
    "test:debug": "jest --inspect-brk --runInBand --no-cache",
    "preview": "vite preview",
    "build:stage": "yarn prebuild && env-cmd -f .env.stage vite build",
    "build:production": "yarn prebuild && env-cmd -f .env.prod vite build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/dompurify": "^3.0.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.2"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "jest-environment-jsdom",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "modulePaths": [
      "<rootDir>/src"
    ]
  }
}
