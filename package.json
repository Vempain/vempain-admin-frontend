{
  "name" : "@vempain/vempain-admin-frontend",
  "version" : "0.9.0",
  "description" : "Vempain Admin frontend",
  "author" : "Paul-Erik Törrönen",
  "homepage" : "https://vempain.poltsi.fi",
  "license" : "GPL-2.0",
  "packageManager" : "yarn@4.12.0",
  "scripts" : {
    "prebuild" : "node generateBuildInfo.js",
    "prestart" : "node generateBuildInfo.js",
    "start" : "env-cmd -f .env.local vite",
    "start-combined" : "env-cmd -f .env.combined vite --port 4000",
    "build" : "yarn prebuild && tsc && env-cmd -f .env.prod vite build",
    "test" : "jest",
    "test:watch" : "jest --watch",
    "test:coverage" : "jest --coverage .",
    "test:debug" : "jest --inspect-brk --runInBand --no-cache",
    "preview" : "vite preview",
    "build:production" : "yarn prebuild && env-cmd -f .env.prod vite build"
  },
  "dependencies" : {
    "@ant-design/icons" : "^6.1.0",
    "@types/dompurify" : "^3.2.0",
    "@types/node" : "^24.10.13",
    "@types/react-highlight-words" : "^0.20.1",
    "@vempain/vempain-auth-frontend" : "^1.0.3",
    "@vitejs/plugin-react" : "^5.1.4",
    "antd" : "^6.3.1",
    "axios" : "^1.13.6",
    "dayjs" : "^1.11.19",
    "dompurify" : "^3.3.1",
    "env-cmd" : "^11.0.0",
    "rc-virtual-list" : "^3.19.2",
    "react" : "^19.2.4",
    "react-dom" : "^19.2.4",
    "react-highlight-words" : "^0.21.0",
    "react-router-dom" : "^7.13.1",
    "web-vitals" : "^5.1.0"
  },
  "devDependencies" : {
    "@eslint/js" : "^10.0.1",
    "@testing-library/dom" : "^10.4.1",
    "@testing-library/jest-dom" : "^6.9.1",
    "@testing-library/react" : "^16.3.2",
    "@testing-library/user-event" : "^14.6.1",
    "@types/jest" : "^30.0.0",
    "@types/react" : "^19.2.14",
    "@types/react-dom" : "^19.2.3",
    "@vitejs/plugin-react-swc" : "^4.2.3",
    "eslint" : "^10.0.2",
    "eslint-plugin-react-hooks" : "^7.0.1",
    "eslint-plugin-react-refresh" : "^0.5.2",
    "globals" : "^17.4.0",
    "jest" : "^30.2.0",
    "jest-environment-jsdom" : "^30.2.0",
    "ts-jest" : "^29.4.6",
    "typescript" : "^5.9.3",
    "typescript-eslint" : "^8.56.1",
    "vite" : "^7.3.1",
    "vite-tsconfig-paths" : "^6.1.1"
  },
  "eslintConfig" : {
    "extends" : [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist" : {
    "production" : [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development" : [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository" : {
    "type" : "git",
    "url" : "git+https://github.com/Vempain/vempain-auth-frontend.git"
  }
}
